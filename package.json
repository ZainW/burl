{
  "name": "burl",
  "version": "0.1.0",
  "description": "Modern HTTP benchmarking CLI tool with rich TUI and LLM-optimized output",
  "type": "module",
  "bin": {
    "burl": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build --compile --minify --sourcemap ./src/index.ts --outfile dist/burl",
    "build:linux-x64": "bun build --compile --target=bun-linux-x64 --minify ./src/index.ts --outfile dist/burl-linux-x64",
    "build:linux-arm64": "bun build --compile --target=bun-linux-arm64 --minify ./src/index.ts --outfile dist/burl-linux-arm64",
    "build:darwin-x64": "bun build --compile --target=bun-darwin-x64 --minify ./src/index.ts --outfile dist/burl-darwin-x64",
    "build:darwin-arm64": "bun build --compile --target=bun-darwin-arm64 --minify ./src/index.ts --outfile dist/burl-darwin-arm64",
    "build:windows-x64": "bun build --compile --target=bun-windows-x64 --minify ./src/index.ts --outfile dist/burl-windows-x64.exe",
    "build:all": "bun run build:linux-x64 && bun run build:linux-arm64 && bun run build:darwin-x64 && bun run build:darwin-arm64 && bun run build:windows-x64",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@opentui/core": "^0.1.66",
    "@opentui/react": "^0.1.66",
    "cleye": "^1.3.2",
    "hdr-histogram-js": "^3.0.0",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "@types/react": "^19.2.7",
    "typescript": "^5.7.2"
  },
  "engines": {
    "bun": ">=1.3.5"
  },
  "license": "MIT-0",
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZainW/burl.git"
  },
  "homepage": "https://github.com/ZainW/burl#readme",
  "bugs": {
    "url": "https://github.com/ZainW/burl/issues"
  },
  "keywords": [
    "http",
    "benchmark",
    "load-testing",
    "cli",
    "tui",
    "bun",
    "performance"
  ]
}
