{
  "name": "@burl/cli",
  "version": "0.2.0",
  "description": "Modern HTTP benchmarking CLI tool with rich TUI and LLM-optimized output",
  "keywords": [
    "benchmark",
    "benchmarking",
    "http",
    "http-benchmark",
    "load-testing",
    "load-test",
    "performance",
    "performance-testing",
    "api-testing",
    "stress-test",
    "cli",
    "tui",
    "terminal",
    "bun",
    "wrk",
    "hey",
    "ab",
    "apache-bench",
    "llm",
    "ai",
    "http2",
    "rest-api",
    "graphql"
  ],
  "homepage": "https://github.com/ZainW/burl#readme",
  "bugs": {
    "url": "https://github.com/ZainW/burl/issues"
  },
  "license": "MIT-0",
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZainW/burl.git",
    "directory": "packages/cli"
  },
  "bin": {
    "burl": "./src/index.ts"
  },
  "type": "module",
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build --compile --minify --sourcemap ./src/index.ts --outfile dist/burl",
    "build:linux-x64": "bun build --compile --target=bun-linux-x64 --minify ./src/index.ts --outfile dist/burl-linux-x64",
    "build:linux-arm64": "bun build --compile --target=bun-linux-arm64 --minify ./src/index.ts --outfile dist/burl-linux-arm64",
    "build:darwin-arm64": "bun build --compile --target=bun-darwin-arm64 --minify ./src/index.ts --outfile dist/burl-darwin-arm64",
    "build:windows-x64": "bun build --compile --target=bun-windows-x64 --minify ./src/index.ts --outfile dist/burl-windows-x64.exe",
    "build:all": "bun run build:linux-x64 && bun run build:linux-arm64 && bun run build:darwin-arm64 && bun run build:windows-x64",
    "test": "bun test",
    "typecheck": "tsgo --noEmit",
    "lint": "oxlint -c .oxlintrc.json --tsconfig tsconfig.json --import-plugin --deny-warnings",
    "lint:fix": "oxlint -c .oxlintrc.json --tsconfig tsconfig.json --fix"
  },
  "dependencies": {
    "@opentui/core": "^0.1.67",
    "@opentui/react": "^0.1.67",
    "cleye": "^2.2.1",
    "hdr-histogram-js": "^3.0.1",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/react": "^19.2.7",
    "@typescript/native-preview": "^7.0.0-dev.20260101.1",
    "oxlint": "^1.36.0"
  }
}
